update  M_FINANCIAL_YEAR set VALIDTO=to_timestamp('31-03-17 12:04:40.000000000 PM','DD-MM-RR HH12:MI:SSXFF AM'),
VALIDFROM=to_timestamp('01-04-16 12:04:40.000000000 PM','DD-MM-RR HH12:MI:SSXFF AM') where M_FINANCIAL_YEAR_ID=1;

update  M_FINANCIAL_YEAR set VALIDTO=to_timestamp('31-03-18 12:04:40.000000000 PM','DD-MM-RR HH12:MI:SSXFF AM'),
VALIDFROM=to_timestamp('01-04-17 12:04:40.000000000 PM','DD-MM-RR HH12:MI:SSXFF AM') where M_FINANCIAL_YEAR_ID=2;

update M_FINANCIAL_YEAR set VALIDTO=to_timestamp('31-03-19 12:04:40.000000000 PM','DD-MM-RR HH12:MI:SSXFF AM'),
VALIDFROM=to_timestamp('01-04-18 12:04:40.000000000 PM','DD-MM-RR HH12:MI:SSXFF AM') where M_FINANCIAL_YEAR_ID=3;